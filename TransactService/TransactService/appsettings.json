{
  "Database": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "ProductStore"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "BootstrapServerKafka": {
    "BootstrapServers": "localhost:9092",
    "GroupId": "transact_group",
    "Topicks": [
      "TransactionBalanceCommitted",
      "TransactionCompleted",
      "TransactionCanceled",
      "BuyOrderCreated",
      "SellOrderCreated",
      "BalanceReplenished"
    ]
  }
}
