{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "KafkaSettings": {
    "BootstrapServers": "localhost:9092",
    "GroupId": "ProductService",
    "Topicks": [
      "BuyOrderCreated",
      "SellOrderCreated",
      "ProductPriceChanged"
    ]
  },
  "MongoSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "ProductDatabase",
    "CollectionName": "Products"
  }

}
