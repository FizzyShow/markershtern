{
  "Database": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "ProductStore"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    },
    "Endpoints": {
      "Http": { "Url": "http://localhost:9001" }
    }
  },
  "BootstrapServerKafka": {
    "BootstrapServers": "localhost:9092",
    "GroupId": "test_group",
    "Topicks": [
      "TransactionBalanceCommitted",
      "TransactionCompleted",
      "TransactionCanceled"
    ]

  }
}
